<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from GestionEmpleadosService WSDL service -->

  <service name="GestionEmpleados" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://example.com"/>
      <parameter name="wsdlServiceElement" value="GestionEmpleadosService"/>
      <parameter name="schemaQualified" value="http://example.com"/>
      <parameter name="wsdlServicePort" value="GestionEmpleados"/>
      <parameter name="className" value="com.example.GestionEmpleadosSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="GestionEmpleados"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="buscar" qname="operNS:buscar" xmlns:operNS="http://example.com" returnQName="retNS:buscarReturn" xmlns:retNS="http://example.com" returnType="rtns:Empleado" xmlns:rtns="http://example.com" soapAction="" >
        <parameter qname="pns:numero" xmlns:pns="http://example.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="mostrarInfo" qname="operNS:mostrarInfo" xmlns:operNS="http://example.com" returnQName="retNS:mostrarInfoReturn" xmlns:retNS="http://example.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:empleado" xmlns:pns="http://example.com" type="tns:Empleado" xmlns:tns="http://example.com"/>
      </operation>
      <parameter name="allowedMethods" value="buscar mostrarInfo"/>

      <typeMapping
        xmlns:ns="http://example.com"
        qname="ns:Empleado"
        type="java:com.example.Empleado"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
