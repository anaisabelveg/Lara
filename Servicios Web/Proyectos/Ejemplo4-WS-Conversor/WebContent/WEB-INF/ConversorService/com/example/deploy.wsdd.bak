<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ConversorService WSDL service -->

  <service name="Conversor" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://example.com"/>
      <parameter name="wsdlServiceElement" value="ConversorService"/>
      <parameter name="schemaQualified" value="http://example.com"/>
      <parameter name="wsdlServicePort" value="Conversor"/>
      <parameter name="className" value="com.example.ConversorSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Conversor"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="ptsEuros" qname="operNS:ptsEuros" xmlns:operNS="http://example.com" returnQName="retNS:ptsEurosReturn" xmlns:retNS="http://example.com" returnType="rtns:double" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:pts" xmlns:pns="http://example.com" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="eurosPts" qname="operNS:eurosPts" xmlns:operNS="http://example.com" returnQName="retNS:eurosPtsReturn" xmlns:retNS="http://example.com" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:euros" xmlns:pns="http://example.com" type="tns:double" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="ptsEuros eurosPts"/>

  </service>
</deployment>
